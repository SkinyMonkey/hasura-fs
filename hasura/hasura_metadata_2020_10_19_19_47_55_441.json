                "name": "permission"
              }
            }
          }
        }
      ],
      "insert_permissions": [
        {
          "role": "user",
          "permission": {
            "check": {
              "_or": [
                {
                  "parent_id": {
                    "_is_null": true
                  }
                },
                {
                  "_and": [
                    {
                      "parent": {
                        "is_folder": {
                          "_eq": true
                        }
                      }
                    },
                    {
                      "_or": [
                        {
                          "parent": {
                            "owner_id": {
                              "_eq": "X-Hasura-User-Id"
                            }
                          }
                        },
                        {
                          "permissions": {
                            "_and": [
                              {
                                "beneficiary_id": {
                                  "_eq": "X-Hasura-User-Id"
                                }
                              },
                              {
                                "role": {
                                  "_in": [
                                    "owner",
                                    "writer"
                                  ]
                                }
                              }
                            ]
                          }
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            "set": {
              "owner_id": "x-hasura-User-Id"
            },
            "columns": [
              "is_folder",
              "metadata",
              "name",
              "parent_id"
            ],
            "backend_only": false
          }
        }
      ],
      "select_permissions": [
